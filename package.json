{
  "name": "@dalibor91/local-store",
  "version": "2.0.1",
  "description": "Front-end local package",
  "license": "MIT",
  "author": "Dalibor Menkovic <dalibormenkovic@gmail.com>",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cache",
    "localstorage",
    "indexeddb",
    "storage",
    "browser",
    "key-value",
    "ttl",
    "expiration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dalibor91/local-store.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "tsx dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fake-indexeddb": "^6.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  }
}